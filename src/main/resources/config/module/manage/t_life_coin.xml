st="account!=null and account!=''">            and a.account=#{account}        </if>        <if test="tel!=null and tel!=''">            and a.tel=#{tel}        </if>      )aa    </select>    <select id="getSumAndCount"  resultType="net.jeeshop.services.manage.lifecoin.bean.LifeCoin">        SELECT sum(coinCount) as sumNum,count(1) as countNum FROM t_lifecoin    </select>    <delete id="delete" parameterType="java.lang.Integer" >        delete from t_lifecoin        where id = #{id,jdbcType=INTEGER}    </delete>    <insert id="insert" parameterType="net.jeeshop.services.manage.lifecoin.bean.LifeCoin" useGeneratedKeys="true" keyProperty="id">      insert into t_lifecoin (id, accountId, coinCount,        coinAddress, isLock, remark        )        values (id, #{accountid}, #{coincount},        #{coinaddress}, #{islock}, #{remark}        )    </insert>    <!--<insert id="insertSelective" parameterType="net.jeeshop.services.manage.lifecoin.bean.LifeCoin" >        insert into t_lifecoin        <trim prefix="(" suffix=")" suffixOverrides="," >            <if test="id != null" >                id,            </if>            <if test="accountid != null" >                accountId,            </if>            <if test="coincount != null" >                coinCount,            </if>            <if test="coinaddress != null" >                coinAddress,            </if>            <if test="islock != null" >                isLock,            </if>            <if test="remark != null" >                remark,            </if>        </trim>        <trim prefix="values (" suffix=")" suffixOverrides="," >            <if test="id != null" >                #{id,jdbcType=INTEGER},            </if>            <if test="accountid != null" >                #{accountid,jdbcType=INTEGER},            </if>            <if test="coincount != null" >                #{coincount,jdbcType=DOUBLE},            </if>            <if test="